<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.mybatics.dao.goo.OrderDao" >

	<insert id="saveOrderInfo" parameterType="string">
    	insert into order_info (phoneNumber,end_time,reason,create_time) values(#{phoneNumber},#{end_time},#{reason},now())
    </insert>

</mapper>